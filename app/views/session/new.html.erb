<div class='body-container'>
  <h1>Log in:</h1>

  <div class='form-container'>
    <%= form_tag("/session", method: "post") do %>
      <%= label_tag(:email) %>
      <%= text_field_tag(:email) %>
      <%= label_tag(:password) %>
      <%= password_field_tag(:password) %>
      <%= submit_tag("Login") %>
    <% end %>
  </div>
</div>
