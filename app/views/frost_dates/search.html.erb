
<div class='body-container'>

  <h1>Enter your lat/long:</h1>

  <h3>Use your exact latitude and longitude for accurate results</h3>

  <div class='form-container'>
    <%= form_tag frost_date_search_path, method: "get" do %>

      <%= label_tag :latitude %>
      <%= text_field_tag :latitude %>
      <%= label_tag :longitude %>
      <%= text_field_tag :longitude %>
      <%= submit_tag :submit %>
    <% end %>
  </div>
</div> <!-- body-container -->
